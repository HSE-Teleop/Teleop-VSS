Vehicle.Speed:
    type: sensor
    datatype: float
    dbc2vss:
        signal: VelocityFb
        interval_ms: 10

Vehicle.Teleop.SteeringAngle:
  type: actuator
  datatype: int16
  vss2dbc:
    signal: SteeringCmd
    transform:
      math: "x + 128"

Vehicle.Teleop.EnginePower:
  type: actuator
  datatype: float
  vss2dbc:
    signal: AccelerationCmd
    transform:
      math: "(x + 1) * 127.5"